{
  "name": "bard-file",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "type": "module",
  "module": "dist/components/index.js",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bard-file/bard-file.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "spec": "NODE_OPTIONS=--experimental-vm-modules stencil test --spec",
    "test": "NODE_OPTIONS=--experimental-vm-modules stencil test --spec --e2e",
    "test.watch": "NODE_OPTIONS=--experimental-vm-modules stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "jest": {
    "extensionsToTreatAsEsm": [".ts", ".tsx"],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "dependencies": {
    "@rails/activestorage": "^7.1.2",
    "@rails/request.js": "^0.0.9",
    "@stencil/core": "^4.8.0",
    "mime-lite": "^1.0.3",
    "rails-request-json": "^0.1.1",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "21.1.1"
  },
  "license": "MIT"
}
